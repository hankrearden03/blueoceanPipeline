pipeline {
  agent any
  stages {
    stage('Build') {
      steps {
        sh 'echo \'Building from Blue Ocean\''
      }
    }
    stage('Test') {
      steps {
        sh 'echo \'Testing from Blue Ocean\''
      }
    }
    stage('Deploy') {
      steps {
        sh 'echo \'deploy from blue ocean\''
      }
    }
  }
}